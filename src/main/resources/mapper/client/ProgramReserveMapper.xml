<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.hospital.client.mapper.ProgramReserveMapper">
	<select id="firstProgram">
		select * from program
		where pro_ju=0
	</select>
	<select id="secondProgram">
		select * from program
		where pro_ju=1
	</select>
	<select id="thirdProgram">
		select * from program
		where pro_ju=2
	</select>
	<select id="fourthProgram">
		select * from program
		where pro_ju=3
	</select>

</mapper>